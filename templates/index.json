                  <div class=\"product-info product-info-top\">\n                        <div class=\"product-subtitle-top\">Kitchenware that fits your world</div>\n                        <div class=\"product-title-top\">Get inspired</div>\n                        <div class=\"product-btns\">\n                            <a class=\"button button--secondary button--sm icon-with-text\" href=\"https://www.anray.com/products/anray-griddle51\" is=\"hover-link\">\n                    <span class=\"btn-fill\" data-fill=\"\" style=\"transform: translateY(var(--motion-translateY)); --motion-translateY: -76%;\"></span>\n                    <span class=\"btn-text\">Shop  Now<svg class=\"icon icon-arrow-right icon-sm transform\" viewBox=\"0 0 21 20\" stroke=\"currentColor\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n      <path stroke-linecap=\"round\" stroke-linejoin=\"round\" d=\"M3 10H18M18 10L12.1667 4.16675M18 10L12.1667 15.8334\"></path>\n    </svg></span>\n                  </a>\n                        </div>\n                    </div>\n                    <div class=\"product-image product-image-top\">\n                        <img src=\"https://cdn.shopify.com/s/files/1/0683/9065/0074/files/zzj.png?v=1745754049\"\n                            alt=\"Portable Juicer\">\n                    </div>\n                </div>\n            </div>\n            <!-- 下方产品（烤面包机） -->\n            <div class=\"product product-bottom\">\n                <div class=\"product-content product-content-bottom\">\n                    <div class=\"product-image product-image-bottom\">\n                        <img src=\"https://cdn.shopify.com/s/files/1/0683/9065/0074/files/mbj.png?v=1745754049\"\n                            alt=\"Toaster\">\n                    </div>\n                    <div class=\"product-info product-info-bottom\">\n                        <div class=\"product-title-bottom\">Kitchenware that fits your world</div>\n                        <a class=\"button button--secondary button--sm icon-with-text\" href=\"https://www.anray.com/products/anray-griddle51\" is=\"hover-link\">\n                    <span class=\"btn-fill\" data-fill=\"\" style=\"transform: translateY(var(--motion-translateY)); --motion-translateY: -76%;\"></span>\n                    <span class=\"btn-text\">Shop  Now<svg class=\"icon icon-arrow-right icon-sm transform\" viewBox=\"0 0 21 20\" stroke=\"currentColor\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n      <path stroke-linecap=\"round\" stroke-linejoin=\"round\" d=\"M3 10H18M18 10L12.1667 4.16675M18 10L12.1667 15.8334\"></path>\n    </svg></span>\n                  </a>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n\n    <script>\n        // 轮播图功能实现\n        const slider = document.querySelector('.slider');\n        const slides = document.querySelectorAll('.slide');\n        const dots = document.querySelectorAll('.pagination-dot');\n        const progressBars = document.querySelectorAll('.progress');\n\n        let currentSlide = 0;\n        const slideInterval = 5000; // 5秒切换一次\n        let slideTimer;\n\n        // 初始化轮播图\n        function initSlider() {\n            // 设置第一张幻灯片为激活状态\n            slides[0].classList.add('active');\n            \n            // 设置分页点击事件\n            dots.forEach((dot, index) => {\n                dot.addEventListener('click', () => {\n                    goToSlide(index);\n                    resetProgressBar();\n                    startProgressBar(index);\n                });\n            });\n            \n            // 添加触摸滑动支持\n            let touchStartX = 0;\n            let touchEndX = 0;\n            \n            slider.addEventListener('touchstart', (e) => {\n                touchStartX = e.changedTouches[0].screenX;\n            });\n            \n            slider.addEventListener('touchend', (e) => {\n                touchEndX = e.changedTouches[0].screenX;\n                handleSwipe();\n            });\n            \n            function handleSwipe() {\n                // 向左滑动 (下一张)\n                if (touchEndX < touchStartX - 50) {\n                    const nextSlide = (currentSlide + 1) % slides.length;\n                    goToSlide(nextSlide);\n                }\n                // 向右滑动 (上一张)\n                if (touchEndX > touchStartX + 50) {\n                    const prevSlide = (currentSlide - 1 + slides.length) % slides.length;\n                    goToSlide(prevSlide);\n                }\n            }\n            \n            // 开始自动轮播\n            startSlideTimer();\n        }\n\n        // 切换到指定幻灯片\n        function goToSlide(index) {\n            // 重置当前进度条\n            resetProgressBar();\n\n            // 移除当前激活的幻灯片\n            slides[currentSlide].classList.remove('active');\n\n            // 更新当前幻灯片索引\n            currentSlide = index;\n\n            // 激活新的幻灯片\n            slides[currentSlide].classList.add('active');\n\n            // 更新分页点状态\n            updateDots();\n\n            // 重新开始计时器\n            startSlideTimer();\n        }\n\n        // 更新分页点状态\n        function updateDots() {\n            dots.forEach((dot, index) => {\n                if (index === currentSlide) {\n                    dot.classList.add('active');\n                    // 开始进度条动画\n                    startProgressBar(index);\n                } else {\n                    dot.classList.remove('active');\n                }\n            });\n        }\n\n        // 开始进度条动画\n        function startProgressBar(index) {\n            const progress = progressBars[index];\n            progress.style.width = '0%';\n\n            // 使用CSS动画实现进度条\n            progress.style.transition = `width ${slideInterval}ms linear`;\n            setTimeout(() => {\n                progress.style.width = '100%';\n            }, 50);\n        }\n\n        // 重置进度条\n        function resetProgressBar() {\n            progressBars.forEach(progress => {\n                progress.style.transition = 'none';\n                progress.style.width = '0%';\n            });\n        }\n\n        // 开始自动轮播计时器\n        function startSlideTimer() {\n            // 清除现有计时器\n            if (slideTimer) {\n                clearTimeout(slideTimer);\n            }\n\n            // 设置新计时器\n            slideTimer = setTimeout(() => {\n                // 计算下一张幻灯片索引\n                const nextSlide = (currentSlide + 1) % slides.length;\n                goToSlide(nextSlide);\n            }, slideInterval);\n        }\n\n        // 初始化轮播图\n        initSlider();\n        // 显示第一张幻灯片的进度条\n        startProgressBar(0);\n    </script>",
