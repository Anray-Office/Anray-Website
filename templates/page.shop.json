      "name": "t:sections.custom-liquid.presets.name",
      "settings": {
        "liquid": "<style>\n        /* 父容器共有样式 */\n        .accessories-section {\n            max-width: 1600px;\n            margin: 0 auto;\nscroll-margin-top: 300px;\n        }\n\n        .products-container-grey {\n            display: flex;\n            flex-wrap: nowrap;\n            gap: 20px;\n            overflow-x: auto;\n            scrollbar-width: none; /* Firefox */\n            -ms-overflow-style: none; /* IE and Edge */\n            scroll-behavior: smooth;\n            -webkit-overflow-scrolling: touch;\n            padding: 10px 0;\n            scroll-snap-type: x mandatory; /* 添加强制吸附滚动 */\n        }\n        \n        /* 隐藏滚动条 */\n        .products-container-grey::-webkit-scrollbar {\n            display: none; /* Chrome, Safari, Opera*/\n        }\n\n        /* 产品卡片链接样式 */\n        .products-container-grey .product-link {\n            text-decoration: none;\n            color: inherit;\n            display: block;\n            flex: 0 0 calc(25% - 15px); /* 一行刚好显示4个，考虑间距 */\n            min-width: calc(25% - 15px);\n            scroll-snap-align: start; /* 卡片左侧与容器左侧对齐 */\n        }\n\n        .products-container-grey .product-card {\n            width: 100%;\n            height: 380px;\n            border-radius: 15px;\n            font-family: Arial, sans-serif;\n            overflow: hidden;\n            display: flex;\n            flex-direction: column;\n            margin-bottom: 20px;\n            cursor: pointer;\n            transition: all 0.3s ease-in-out;\n        }\n\n        .products-container-grey .product-card:hover .product-image img {\n            transform: scale(1.05);\n        }\n\n        /* 响应式设计：不同屏幕尺寸下的布局调整 */\n        @media (max-width: 1200px) {\n            .accessories-section {\n                padding: 0 20px;\n            }\n            \n            .products-container-grey .product-link {\n                flex: 0 0 calc(33.333% - 14px); /* 一行显示3个 */\n                min-width: calc(33.333% - 14px);\n            }\n        }\n        \n        @media (max-width: 992px) {\n            .products-container-grey .product-link {\n                flex: 0 0 calc(50% - 10px); /* 一行显示2个 */\n                min-width: calc(50% - 10px);\n            }\n            \n            .accessories-title-container .accessories-title {\n                font-size: 2rem;\n            }\n        }\n        \n        @media (max-width: 576px) {\n            .accessories-section {\n                padding: 0 15px;\n            }\n            \n            .products-container-grey .product-link {\n                flex: 0 0 calc(100% - 5px); /* 一行显示1个 */\n                min-width: calc(100% - 5px);\n            }\n            \n            .accessories-title-container {\n                flex-direction: column;\n                align-items: flex-start;\n                margin-bottom: 20px;\n            }\n            \n            .accessories-title-container .accessories-title {\n                font-size: 1.8rem;\n                margin-bottom: 10px;\n            }\n            \n            .accessories-title-container .see-all {\n                margin-left: 0;\n            }\n            \n            .products-container-grey .product-card {\n                height: 340px;\n            }\n        }\n\n        .products-container-grey .product-image {\n            flex: 1;\n            padding: 0 0.75rem;\n            overflow: hidden;\n            background-color: #eeeeee;\n        }\n\n        .products-container-grey .product-image img {\n            width: 100%;\n            height: 100%;\n            object-fit: contain;\n            display: block;\n            transition: all 0.3s ease;\n        }\n\n        .products-container-grey .product-image-radius {\n            border-radius: 15px;\n        }\n\n        .products-container-grey .product-info {\n            padding: 20px 5px;\n            display: flex;\n            flex-direction: column;\n            justify-content: space-between;\n        }\n\n        .products-container-grey .product-details-row {\n            display: flex;\n            justify-content: space-between;\n            align-items: flex-start;\n            width: 100%;\n            margin-bottom: 15px;\n        }\n\n        .products-container-grey .product-name {\n            font-size: 1.3rem;\n            font-weight: bold;\n            color: #333;\n            margin: 0 0 5px 0;\n        }\n\n        .products-container-grey .product-price-value {\n            margin: 5px 0 0 0;\n        }\n\n        .products-container-grey .product-description {\n            font-size: 1rem;\n            color: #777;\n            margin: 0;\n        }\n\n        .products-container-grey .product-options {\n            display: flex;\n            flex-direction: column;\n            align-items: flex-end;\n        }\n\n        .products-container-grey .product-price {\n            font-size: 1.3rem;\n            font-weight: bold;\n            color: #333;\n            text-align: right;\n        }\n\n        .products-container-grey .product-price .from-text {\n            font-size: 1rem;\n            font-weight: normal;\n            color: #555;\n            display: block;\n        }\n\n        /* 标题容器样式 */\n        .accessories-title-container {\n            display: flex;\n            justify-content: flex-start;\n            align-items: center;\n            margin: 0 0 30px 0;\n        }\n        \n        .accessories-title-container .accessories-title {\n            font-size: 2.5rem;\n            font-weight: bold;\n            color: #000;\n            margin: 0;\n            line-height: 1;\n        }\n        \n        .accessories-title-container .see-all {\n            color: #000;\n            text-decoration: none;\n            font-size: 1rem;\n            margin-left: 100px;\n            align-self: flex-end;\n            position: relative;\n        }\n        \n        .accessories-title-container .see-all::after